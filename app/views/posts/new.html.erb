<h1>New Post</h1>
<h3>Post Form</h3>

    <fieldset class="">
      <legend>Enter Overtime information:</legend>
      <%= form_for @post do |f| %>
        <%= f.date_field :date %>
        <%= f.text_area :rationale %>
        <%= f.submit 'Submit Post' %>
      <% end %>
    </fieldset>
<%= params.inspect %>